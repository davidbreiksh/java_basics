<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
    <Appenders>

        <File name="NotImageLogger" fileName="logs/search.log">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </File>


    </Appenders>

    <Loggers>
        <Root level="off"/>

        <Logger name="NotImageLogger" level="info" additivity="false">
            <AppenderRef ref="NotImage"/>
        </Logger>

    </Loggers>

</configuration>
